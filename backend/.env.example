# Copy this file to `.env` and fill in real values for local development.

# MySQL database connection
DB_HOST=127.0.0.1
DB_USER=root
DB_PASSWORD=
DB_NAME=rodela_brand

# Where your Next.js app runs (used for CORS and redirects)
FRONTEND_URL=http://localhost:3000

# Express session secret (change to something random in production)
SESSION_SECRET=supersecret

# Google OAuth credentials (create via Google Cloud Console)
GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your-google-client-secret
GOOGLE_CALLBACK_URL=http://localhost:5000/auth/google/callback